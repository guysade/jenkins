apiVersion: networking.k8s.io/v1
  kind: Ingress
  metadata:
    name: mongodb-express-ingress
    labels:
        name: mongodb-express-ingress
  spec:
    rules:
    - host: mongodb-express-ingress
      http:
        paths:
        - pathType: Prefix
          path: "/"
          backend:
            service:
              name: mongo-express
              port: 
                number: 8081
